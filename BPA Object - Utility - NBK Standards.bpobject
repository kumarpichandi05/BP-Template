  <stage stageid="bf049a69-da2c-47ac-82c2-b923282600b5" name="CheckFolderAndFileExist" type="Code">
    <subsheetid>bff49792-29f0-4a41-9f14-76955b1fc8ef</subsheetid>
    <loginhibit />
    <display x="15" y="0" />
    <inputs>
      <input type="text" name="folderPath" expr="[Folder Path]" />
      <input type="text" name="filePath" expr="[File Path]" />
    </inputs>
    <outputs>
      <output type="text" name="message" stage="message" />
    </outputs>
    <onsuccess>ecedbf55-6b95-4c44-a77f-d3886ecf2b84</onsuccess>
    <code><![CDATA[message = "";

try
{
    // Check if folder exists
    if (Directory.Exists(folderPath))
    {
        message = "Folder exists.";

        // Check if file exists inside folder
        if (File.Exists(filePath))
        {
            message = "File exists inside folder.";
        }
        else
        {
            message = "File does NOT exist inside folder.";
        }
    }
    else
    {
        message = "Folder does NOT exist.";
    }
}
catch (Exception ex)
{
    message = "Error checking folder or file: " + ex.Message;
}]]></code>
  </stage>
