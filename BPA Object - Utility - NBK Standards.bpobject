  <stage stageid="ab6ec5c9-ab33-4bb9-93e2-f010d5bb7bd0" name="Read Json old" type="Code">
    <subsheetid>2af6258a-7a02-44a0-ae9e-c03eed5a381a</subsheetid>
    <loginhibit />
    <display x="15" y="0" />
    <inputs>
      <input type="text" name="ENVJsonText" expr="[ENVJsonText]" />
      <input type="collection" name="TemplateCollection" expr="[TemplateCollection]" />
      <input type="text" name="FilePathJsonText" expr="[FilePathJsonText]" />
      <input type="text" name="Source" expr="[Source]" />
    </inputs>
    <outputs>
      <output type="collection" name="OutputCollection" stage="OutputCollection" />
      <output type="text" name="message" stage="message" />
    </outputs>
    <onsuccess>a92c7a65-0f64-41b2-99ee-78efda59b8f7</onsuccess>
    <code><![CDATA[message = "";

try
{
    string jsonContent;

    // ---------- Determine source ----------
    if (Source != "ENV")
    {
        if (!File.Exists(FilePathJsonText))
        {
            message = $"JSON file does not exist: {FilePathJsonText}";
            OutputCollection = TemplateCollection.Clone();
            return;
        }
        jsonContent = File.ReadAllText(FilePathJsonText);
    }
    else
    {
        jsonContent = ENVJsonText;
    }

    // ---------- Clean and normalize JSON ----------
    jsonContent = jsonContent.Trim();

    // If multiple objects not wrapped in array, wrap them
    if (!jsonContent.StartsWith("["))
    {
        // Split by "}," safely to detect multiple objects
        string[] objects = jsonContent.Split(new string[] { "}," }, StringSplitOptions.RemoveEmptyEntries);
        if (objects.Length > 1)
        {
            for (int i = 0; i < objects.Length; i++)
            {
                if (!objects[i].EndsWith("}"))
                    objects[i] += "}";
            }
            jsonContent = "[" + string.Join(",", objects) + "]";
        }
    }

    // ---------- Parse JSON ----------
    JToken token = JToken.Parse(jsonContent);
    JArray jsonArray = token.Type == JTokenType.Array ? (JArray)token : new JArray(token);

    // ---------- Prepare output collection ----------
    DataTable dt = TemplateCollection.Clone();

    // ---------- Fill collection ----------
    foreach (JObject obj in jsonArray)
    {
        DataRow row = dt.NewRow();

        foreach (DataColumn col in dt.Columns)
        {
            // Case-insensitive lookup
            if (obj.TryGetValue(col.ColumnName, StringComparison.OrdinalIgnoreCase, out JToken value))
            {
                row[col.ColumnName] = value.ToString();
            }
        }

        dt.Rows.Add(row);
    }

    OutputCollection = dt;
    message = "Success";
}
catch (Exception ex)
{
    message = "Error parsing JSON: " + ex.Message;
    OutputCollection = TemplateCollection.Clone();
}
]]></code>
  </stage>
