  <stage stageid="be4c4460-1de5-45a3-91e5-930419338674" name="Delete Files" type="Code">
    <subsheetid>6279e614-689b-413f-98c4-8b46c9f2e4d0</subsheetid>
    <loginhibit />
    <display x="15" y="0" />
    <inputs>
      <input type="text" name="folderPath" expr="[folderPath]" />
      <input type="text" name="fileExtension" expr="[fileExtension]" />
      <input type="number" name="fileCycleDays" expr="[fileCycleDays]" />
    </inputs>
    <outputs>
      <output type="text" name="message" stage="message" />
    </outputs>
    <onsuccess>e4051277-87bc-42e8-beee-adca4d6630f2</onsuccess>
    <code><![CDATA[message = "";

try
{
    // --------- Validate Inputs ----------
    if (string.IsNullOrWhiteSpace(folderPath))
    {
        message = "Folder path is empty.";
        return;
    }

    if (!Directory.Exists(folderPath))
    {
        message = "Folder does not exist: " + folderPath;
        return;
    }

    if (string.IsNullOrWhiteSpace(fileExtension))
    {
        fileExtension = "*"; // all files
    }
    else
    {
        fileExtension = fileExtension.TrimStart('.'); // remove dot if provided
    }

     // --------- Determine date threshold ----------
    // Convert decimal input to double
    double days = Convert.ToDouble(fileCycleDays);
    DateTime thresholdDate = DateTime.Now.AddDays(-days); // delete files older than threshold

    // --------- Get files matching extension ----------
    string searchPattern = fileExtension == "*" ? "*.*" : "*." + fileExtension;
    string[] files = Directory.GetFiles(folderPath, searchPattern);

    int deletedCount = 0;

    foreach (string file in files)
    {
        DateTime lastWrite = File.GetLastWriteTime(file);

        if (lastWrite <= thresholdDate)
        {
            File.Delete(file);
            deletedCount++;
        }
    }

    message = $"Deleted {deletedCount} file(s) from folder: {folderPath}";
}
catch (Exception ex)
{
    message = "Error deleting files: " + ex.Message;
}]]></code>
