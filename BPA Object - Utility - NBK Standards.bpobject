  <stage stageid="adab2b8f-fc2c-41d1-9c1e-0021c67097c9" name="Custom Log" type="Code">
    <subsheetid>92e79722-db22-463e-9ba4-bfbb337b6a6e</subsheetid>
    <loginhibit />
    <display x="15" y="0" />
    <inputs>
      <input type="text" name="logBaseFolder" expr="[logBaseFolder]" />
      <input type="text" name="logLevel" expr="[logLevel]" />
      <input type="text" name="logMessage" expr="[logMessage]" />
    </inputs>
    <outputs>
      <output type="text" name="message" stage="message" />
      <output type="text" name="logFolder" stage="logFolder" />
