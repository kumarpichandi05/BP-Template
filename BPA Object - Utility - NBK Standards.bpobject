    <onsuccess>ee4c4d7b-1613-495a-96a6-32f293f8ea9c</onsuccess>
    <code><![CDATA[message = "";
logFolder = ""; // Output: full daily folder path

try
{
    // --------- Validate Inputs ----------
    if (string.IsNullOrWhiteSpace(logBaseFolder))
    {
        message = "Invalid base log folder path.";
        return;
    }

    if (string.IsNullOrWhiteSpace(logMessage))
    {
        logMessage = "(No message provided)";
    }

    // Validate log level, default to INFO if empty/invalid
    string[] validLevels = new string[] { "INFO", "WARN", "ERROR" };
    if (string.IsNullOrWhiteSpace(logLevel) || Array.IndexOf(validLevels, logLevel.ToUpper()) < 0)
    {
        logLevel = "INFO";
    }
    else
    {
        logLevel = logLevel.ToUpper();
    }

    // --------- Create Daily Folder ----------
    logFolder = Path.Combine(logBaseFolder, DateTime.Now.ToString("yyyy-MM-dd"));
    if (!Directory.Exists(logFolder))
    {
        Directory.CreateDirectory(logFolder);
    }

    // --------- Build dynamic log file name ----------
    // Format: BOT_Log_yyyy-MM-dd.txt
    string logFileName = $"BOT_Log_{DateTime.Now:yyyy-MM-dd}.txt";
    string logFilePath = Path.Combine(logFolder, logFileName);

    // --------- Build log entry with timestamp and level ----------
    string logEntry = $"{DateTime.Now:yyyy-MM-dd HH:mm:ss} | {logLevel} | {logMessage}{Environment.NewLine}";

    // --------- Append log entry to file ----------
    File.AppendAllText(logFilePath, logEntry);

    message = $"Log written successfully in folder: {logFolder}, file: {logFileName}";
}
catch (Exception ex)
{
    message = "Error writing log: " + ex.Message;
}]]></code>
