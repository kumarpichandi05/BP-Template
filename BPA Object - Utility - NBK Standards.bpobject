  <stage stageid="756e87c6-918b-4fd9-8582-ceccdb0c5775" name="Get Json Value" type="Code">
    <subsheetid>799746ef-c57c-4fc1-8591-773792852474</subsheetid>
    <loginhibit />
    <display x="15" y="0" />
    <inputs>
      <input type="text" name="ENVJsonText" expr="[ENVJsonText]" />
      <input type="text" name="FilePathJsonText" expr="[FilePathJsonText ]" />
      <input type="text" name="Source" expr="[Source]" />
      <input type="collection" name="TemplateCollection" expr="[TemplateCollection]" />
    </inputs>
    <outputs>
      <output type="collection" name="OutputCollection" stage="OutputCollection" />
      <output type="text" name="message" stage="message" />
    </outputs>
    <onsuccess>62dcf8e2-f0c4-4499-873f-04eaa6d5d7af</onsuccess>
    <code><![CDATA[message = "";

try
{
    string jsonContent;

    // Determine source of JSON
    if (Source != "ENV")
    {
        // Validate file path
        if (!File.Exists(FilePathJsonText))
        {
            message = $"JSON file does not exist: {FilePathJsonText}";
            OutputCollection = TemplateCollection.Clone();
            return;
        }

        // Read JSON from file
        jsonContent = File.ReadAllText(FilePathJsonText);
    }
    else
    {
        // JSON is passed directly as string
        jsonContent = ENVJsonText;
    }

    // Parse JSON
    JObject root = JObject.Parse(jsonContent);
    JArray screens = (JArray)root["Screens"];

    // Prepare output DataTable
    DataTable dt = TemplateCollection.Clone();

    // Fill DataTable from JSON
    foreach (JObject screen in screens)
    {
        string screenName = (string)screen["ScreenName"];
        JArray elements = (JArray)screen["Elements"];

        foreach (JObject element in elements)
        {
            DataRow row = dt.NewRow();
            row["ScreenName"] = screenName;
            row["ElementType"] = (string)element["ElementType"];
            row["ElementID"] = (string)element["ElementID"];
            row["WaitTime"] = (int)element["WaitTime"];
            dt.Rows.Add(row);
        }
    }

    // Assign output
    OutputCollection = dt;
    message = "Success";
}
catch (Exception ex)
{
    message = "Error parsing JSON: " + ex.Message;
    OutputCollection = TemplateCollection.Clone();
}
]]></code>
