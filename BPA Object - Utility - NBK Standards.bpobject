  <stage stageid="be4e3e72-f200-4ffb-b01f-e2e6d10f04b6" name="Read Json" type="Code">
    <subsheetid>62625606-2978-47b5-8975-b292f96320eb</subsheetid>
    <loginhibit />
    <display x="15" y="0" />
    <inputs>
      <input type="text" name="ENVJsonText" expr="[ENVJsonText]" />
      <input type="text" name="FilePathJsonText" expr="[FilePathJsonText]" />
      <input type="text" name="Source" expr="[Source]" />
    </inputs>
    <outputs>
      <output type="collection" name="OutputCollection" stage="OutputCollection" />
      <output type="text" name="message" stage="message" />
    </outputs>
    <onsuccess>52a4a8e6-7de4-4cbc-a169-252aba491938</onsuccess>
    <code><![CDATA[message = "";

try
{
    string jsonContent;

    // ---------- Determine source ----------
    if (Source != "ENV")
    {
        if (!File.Exists(FilePathJsonText))
        {
            message = $"JSON file does not exist: {FilePathJsonText}";
            OutputCollection = new DataTable(); // Empty collection
            return;
        }
        jsonContent = File.ReadAllText(FilePathJsonText);
    }
    else
    {
        jsonContent = ENVJsonText;
    }

    // ---------- Clean and normalize JSON ----------
    jsonContent = jsonContent.Trim();

    // Wrap single objects into an array
    if (!jsonContent.StartsWith("["))
    {
        jsonContent = "[" + jsonContent + "]";
    }

    // ---------- Parse JSON ----------
    JArray jsonArray = JArray.Parse(jsonContent);

    // ---------- Create DataTable dynamically ----------
    DataTable dt = new DataTable();

    // Add columns based on all keys in all objects (to be safe)
    foreach (JObject obj in jsonArray)
    {
        foreach (var prop in obj.Properties())
        {
            if (!dt.Columns.Contains(prop.Name))
            {
                dt.Columns.Add(prop.Name);
            }
        }
    }

    // ---------- Fill DataTable ----------
    foreach (JObject obj in jsonArray)
    {
        DataRow row = dt.NewRow();
        foreach (var prop in obj.Properties())
        {
            row[prop.Name] = prop.Value.ToString();
        }
        dt.Rows.Add(row);
    }

    // ---------- Output ----------
    OutputCollection = dt;
    message = "Success";
}
catch (Exception ex)
{
    message = "Error parsing JSON: " + ex.Message;
    OutputCollection = new DataTable(); // Empty collection
}
]]></code>
